ACTION=="move", SUBSYSTEM=="net", KERNEL=="ens*", RUN+="/lib/udev/trigger-interface $env{INTERFACE}"
ACTION=="add", SUBSYSTEM=="net", KERNEL=="ens*", RUN+="/usr/sbin/ec2ifup $env{INTERFACE}"
ACTION=="remove", SUBSYSTEM=="net", KERNEL=="ens*", RUN+="/usr/sbin/ec2ifdown $env{INTERFACE}"
